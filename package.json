    "test-endpoint": "ts-node src/scripts/testEndpoint.ts"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.13.23",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.0",
    "morgan": "^1.10.0",
    "winston": "^3.9.0",
    "zod": "^4.0.0"
  },
